generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Theme {
  id        String   @id @default(cuid())
  name      String   @unique
  values    Json
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  preferences Json?
}

model Embedding {
  id        String   @id @default(cuid())
  content   String
  vector    Float[]
  category  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
